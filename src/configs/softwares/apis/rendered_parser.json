{
    "name": "apis-irestful-rendered-parsers",
    "parents": [
        "http://code.irestful.com/configs/parent/crud.json"
    ],
    "environment_variables": {
        "container_name": "RENDERED_PARSERS_CONTAINER_NAME"
    },
    "routes": {
        "http": [
            {
                "pattern": "/",
                "method": "post",
                "rest_api": "insert_element"
            },
            {
                "pattern": "/*uuid*",
                "method": "get",
                "rest_api": "retrieve_element_by_uuid"
            },
            {
                "pattern": "/*uuid*",
                "method": "delete",
                "rest_api": "delete_element"
            }
        ]
    }
}
